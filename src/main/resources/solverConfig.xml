<?xml version="1.0" encoding="UTF-8"?>
<solver xmlns="https://www.optaplanner.org/xsd/solver"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="https://www.optaplanner.org/xsd/solver https://www.optaplanner.org/xsd/solver/solver.xsd">

    <solutionClass>com.example.transporter.optaplanner.AssignmentSolution</solutionClass>
    <entityClass>com.example.transporter.optaplanner.LaneAssignment</entityClass>

    <scoreDirectorFactory>
        <constraintProviderClass>com.example.transporter.optaplanner.AssignmentConstraintProvider</constraintProviderClass>
    </scoreDirectorFactory>

    <termination>
        <secondsSpentLimit>60</secondsSpentLimit>
    </termination>

    <environmentMode>FULL_ASSERT</environmentMode>
</solver>
